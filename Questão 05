    printf("4\t   16\t        64\n");
    printf("5\t   25\t        125\n");
    printf("6\t   36\t        216\n");
    printf("7\t   49\t        343\n");
    printf("8\t   64\t        512\n");
    printf("9\t   81\t        729\n");
