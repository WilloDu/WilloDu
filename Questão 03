#include <stdio.h>
#include <math.h>

int main()
{   
    int n1;
    int n2;
    float Soma;
    float Prd;
    float Dif;
    float Quo;
    float Mod;
    printf("Insira o primeiro número:");
    scanf ("%d", &n1 );
    printf("Insira o segundo número:");
    scanf ("%d", &n2 );
    Soma= n1 + n2;
    Prd= n1 * n2;
    Dif= n1 - n2;
    Quo= n1 / n2;
    Mod= n1 % n2;
    printf("Soma: %f \n", Soma); 
    printf("Produto: %f \n", Prd);
    printf("Diferença: %f \n", Dif);
    printf("Quociente:%f \n", Quo);
    printf("Módulo: %f \n", Mod);

    return 0;
}
